{
    "version": 2,
    "builds": [
        {
            "src": "api/analyze.js",
            "use": "@vercel/node"
        },
        {
            "src": "client/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/analyze",
            "destination": "/api/analyze.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}